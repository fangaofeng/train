    "node-gyp": "^6.1.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "object-hash": "^2.0.3",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.4.0",
    "pdfjs-dist": "^2.2.228",
    "prop-types": "^15.5.10",
    "qs": "^6.5.2",
    "rc-animate": "^2.4.4",
    "react": "^16.11.0",
    "react-container-query": "^0.11.0",
