    "browserslist": "^4.9.1",
    "caniuse-lite": "^1.0.30001031",
    "classnames": "^2.2.6",
    "dequal": "^1.0.0",
    "dva": "^2.6.0-beta.19",
    "enquire-js": "^0.2.1",
    "hash.js": "^1.1.5",
    "highlight.js": "^9.15.6",
    "immutable": "^3.7.6",
